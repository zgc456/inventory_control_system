<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhkj.inventory_control_dao.entity.CommodityEntity" table="commodity" schema="inventory_control">
        <property name="id" column="id"/>
        <property name="commodityName" column="commodityName"/>
        <property name="commodityIntroduce" column="commodityIntroduce"/>
        <property name="commodityBigPictureUrl" column="commodityBigPictureUrl"/>
        <property name="commodityRelationId" column="commodityRelationId"/>
    </class>
</hibernate-mapping>