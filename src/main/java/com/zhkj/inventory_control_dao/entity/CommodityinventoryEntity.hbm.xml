<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhkj.inventory_control_dao.entity.CommodityinventoryEntity" table="commodityinventory"
           schema="inventory_control">
        <property name="id" column="id"/>
        <property name="commodityNumber" column="commodityNumber"/>
        <property name="commoditySecurityLine" column="commoditySecurityLine"/>
        <property name="commodityPrice" column="commodityPrice"/>
        <property name="commoditySmallPictureUrl" column="commoditySmallPictureUrl"/>
        <property name="commoditySku" column="commoditySku"/>
        <property name="commodityId" column="commodityId"/>
    </class>
</hibernate-mapping>