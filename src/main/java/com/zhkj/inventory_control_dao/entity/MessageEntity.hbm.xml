<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhkj.inventory_control_dao.entity.MessageEntity" table="message" schema="inventory_control">
        <property name="id" column="id"/>
        <property name="messageTitle" column="messageTitle"/>
        <property name="messageContent" column="messageContent"/>
        <property name="messageStatus" column="messageStatus"/>
        <property name="messageTypeId" column="messageTypeId"/>
        <property name="messageCreateTime" column="messageCreateTime"/>
    </class>
</hibernate-mapping>