<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhkj.inventory_control_dao.entity.StatisticsEntity" table="statistics" schema="inventory_control">
        <property name="id" column="id"/>
        <property name="statisticsCreateTime" column="statisticsCreateTime"/>
        <property name="statisticsTypeId" column="statisticsTypeId"/>
        <property name="commodityId" column="commodityId"/>
        <property name="financePrice" column="financePrice"/>
        <property name="financeTypeId" column="financeTypeId"/>
        <property name="statisticsNumber" column="statisticsNumber"/>
    </class>
</hibernate-mapping>